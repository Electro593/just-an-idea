{
  "comments": {
    "lineComment": "//",
    "blockComment": [ "/*", "*/" ]
  },
  "brackets": [
    [ "(", ")" ],
    [ "[", "]" ],
    [ "{", "}" ],
    [ "/*", "*/" ]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "/*", "close": "*/", "notIn": ["string"] },
    { "open": "\"", "close": "\"", "notIn": ["string", "comment"] }
  ],
  "colorizedBracketPairs": [
    [ "(", ")" ],
    [ "[", "]" ],
    [ "{", "}" ]
  ],
  "autoCloseBefore": ";,}]) \n\t",
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "folding": {
    "offSide": false
  },
  "_wordPattern": "[a-zA-Z_](?:\\w|\\\\ *)*|[\\-+]?(?:\\.\\d[\\d_]*|0_*[xb][\\d_]*|\\d[\\d_]*\\.?[\\d_]*)|<<<=|>>>=|<<<|>>>|<<=|>>=|&&=|\\|\\|=|\\/\\*|\\*\\/|\\/\\/|\\[\\]|->|<<|>>|<=|>=|==|!=|&&|\\|\\||\\.\\*|\\+=|-=|\\*=|\\/=|%=|&=|\\|=|\\^=|[~`!@#%^&*()[\\]{}\\-+=|;:\"<>,.?\\/\\\\]",
  "indentationRules": {
    "increaseIndentPattern": "[{\\[(]$",
    "decreaseIndentPattern": "^((?<!\\{[^}]*)\\}|(?<!\\[[^\\]]*)\\]|(?<!\\([^)]*)\\))$",
    "indentNextLinePattern": "(if|for|while).*[^;]$"
  },
  "onEnterRules": []
}